{
  "expo": {
    "name": "thefilter",
    "slug": "thefilter",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "scheme": "mzeroes",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f1f1"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 60000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "io.github.mzeroes.thefilter"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.github.mzeroes.thefilter"
    }
  }
}
